<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
      <servlet-name>CustomerLine</servlet-name>
      <servlet-class>presentation_layer.CustomerLineServlet</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>CustomerLine</servlet-name>
      <url-pattern>/CustomerLineServlet</url-pattern>
  </servlet-mapping>
  <servlet>
      <servlet-name>CustomerDetailServlet</servlet-name>
      <servlet-class>presentation_layer.CustomerDetailServlet</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>CustomerDetailServlet</servlet-name>
      <url-pattern>/CustomerDetailServlet</url-pattern>
  </servlet-mapping>
</web-app>
